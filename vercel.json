{
  "version": 2,
  "routes": [
    {
      "src": "/posts/(.*)",
      "dest": "/public/posts/$1"
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*).(js|css|map|ico|png|jpg|jpeg|svg|webp|woff|woff2|ttf|eot|otf|gif)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
